import java.io.*;
import java.util.*;
public class GreenEnough {
	static int n;
	static int[][] pasture;
	static boolean [][] visited;
	static Scanner in;
	public static void main(String[] args) {
		in = new Scanner(System.in);
		n = in.nextInt();
		pasture = new int[n][n];
		visited = new boolean[n][n];
		for (int i=0; i<n; i++) {
			for (int j=0; j<n; j++) {
				pasture[i][j] = in.nextInt();
			}
		}
		
		for (int i=0; i<n; i++) {
			for (int j=0; j<n; j++) {
				if (pasture[i][j]==100) {
					floodfill(i, j, new ArrayList<CowLoc2>());
				}
			}
		}
	}
	public static ArrayList<CowLoc2> floodfill(int x, int y, ArrayList<CowLoc2> result) {
		if (x<0||x>=n||y<0||y>=n) return result;
		if (pasture[x][y]<100) return result;
		if (visited[x][y]) return result;
		visited[x][y] = true;
		System.out.println(pasture[x][y]);
		result.add(new CowLoc2(x, y));
		floodfill(x+1, y, result);
		floodfill(x-1, y, result);
		floodfill(x, y+1, result);
		floodfill(x, y-1, result);
		
		return result;
	}
	
	
}
class CowLoc2 {
	int x, y;
	public CowLoc2(int x, int y) {
		this.x = x;
		this.y = y;
	}
}
