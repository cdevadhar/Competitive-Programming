import java.util.ArrayList;
public class UniqCheck {
	public boolean verify(String candidate) {
		
		for (int i = 0; i<candidate.length(); i++) {
			for (int j = i+1; i<candidate.length(); j++) {
				if (candidate.charAt(i) == candidate.charAt(j)) {
					return false;
				}
			}
		}
		return true;
	}
	public static void main(String[] args) {
		UniqCheck wordCheck = new UniqCheck();
		System.out.println(wordCheck.verify("abcd"));
	}

}
