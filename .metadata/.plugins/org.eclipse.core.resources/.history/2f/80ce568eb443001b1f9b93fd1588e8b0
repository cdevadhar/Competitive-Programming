import java.io.*;
import java.util.*;
public class Rut {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int N = scan.nextInt();
		for (int i=0; i<N; i++) {
			String direction = scan.next();
			int x = scan.nextInt();
			int y = scan.nextInt();
			System.out.print(direction + " " + x + " " + y);
		}
	}
}

