
public class Queue {
	private static class Node {
		private int value;
		private Node next;
		private Node(int value) {
			this.value = value;
		}
	}
	private Node head;
	private Node tail;
	
	public int peek() {
		return head.value;
	}
	
	public void addToTail(int value) {
		Node newNode = new Node(value);
		if (tail != null) {
			newNode = tail.next;
		}
		tail = newNode;
		
		if (head == null) {
			head = newNode;
		}
	}
	//Removes from the head of the linked list (Because queues are first in first out)
	public void remove() {
		head = head.next;
		if (head == null) {
			tail = null;
		}
	}
	
	public static void main(String[] args) {
		Queue testQueue = new Queue();
		testQueue.addToTail(1);
		testQueue.addToTail(1);
		testQueue.addToTail(2);
		testQueue.addToTail(3);
		testQueue.addToTail(4);
		testQueue.addToTail(5);
		
		System.out.println(testQueue.peek());
		
	}

}
