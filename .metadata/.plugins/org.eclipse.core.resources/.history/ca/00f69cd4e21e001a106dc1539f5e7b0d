
public class Team {
	private Student studentA;
	private Student studentB;
	private String name;
	public Team(Student studentA, Student studentB, String name) {
		if (studentA.getClub() != studentB.getClub()) {
			throw new RuntimeException("Clubs do not match");
		}
		this.studentA = studentA;
		this.studentB = studentB;
		this.name = name;
	}
	public String toString() {
		return "Team: " + name;
	}

}
