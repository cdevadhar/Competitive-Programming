import java.io.*;
import java.util.*;
public class TwoPointers {
	static int n, x;
	static int[] numbers;
	static Scanner in;
	public static void main(String[] args) {
		in = new Scanner(System.in);
		n = in.nextInt();
		x = in.nextInt();
		numbers = new int[n];
		for (int i=0; i<n; i++) {
			numbers[i] = in.nextInt();
		}
		sum2(x, numbers);
		sum3();
		
	}
	
	public static void sum2(int sum, int[] numbers) {
		
		int left = 0;
		int right = n-1;
		while (right>left) {
			if (numbers[left] + numbers[right]>sum) {
				right--;
			}
			if (numbers[left] + numbers[right]<sum) {
				left++;
			}
			else {
				System.out.println((left+1) + " " + (right+1));
				break;
			}
		}
	}
	public static void sum3() {
		int middle=0;
		for (int i=0; i<n; i++) {
			middle = i;
			int sum = x-middle;
			for (int j=i; j<n-1; j++) {
				sum2(sum, Arrays.copyOfRange(numbers, i, n-1));
			}
			
			
		}
	}
	
}
