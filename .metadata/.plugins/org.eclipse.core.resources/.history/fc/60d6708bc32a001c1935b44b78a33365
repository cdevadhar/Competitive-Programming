package labs_projects;

public class Unit2 {
	public static void main(String[] args) {
		Student sw = new Student("Snow White", 1001);
		Student hd = new Student("Humpty Dumpty", 10012);
		Student pc = new Student("Prince Charming", 10023);
		Student sb = new Student("Sleeping Beauty", 1004, 90);
		Student hp = new Student("Henny Penny", 1005, 80);
		System.out.println("Name: " + sw.getFirstName() +"    ID: " + sw.getStudentID() + "     Grade: " + sw.getGrade());
		System.out.println("Name: " + hd.getFirstName() +"    ID: " + hd.getStudentID() + "     Grade: " + hd.getGrade());
		System.out.println("Name: " + pc.getFirstName() +"    ID: " + pc.getStudentID() + "     Grade: " + pc.getGrade());
		System.out.println("Name: " + sb.getFirstName() +"    ID: " + sb.getStudentID() + "     Grade: " + sb.getGrade());
		System.out.println("Name: " + hp.getFirstName() +"    ID: " + hp.getStudentID() + "     Grade: " + hp.getGrade());
	}
}
