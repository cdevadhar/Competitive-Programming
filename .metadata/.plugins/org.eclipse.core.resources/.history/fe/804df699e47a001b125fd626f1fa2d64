import java.io.*;
import java.util.*;
public class YearofCow {
	static int n, k;
	static int[] ancestors;
	static Scanner in;
	public static void main(String[] args) {
		in = new Scanner(System.in);
		n = in.nextInt();
		k = in.nextInt();
		ancestors = new int[n];
		for (int i=0; i<n; i++) {
			ancestors[i] = -1*in.nextInt();
		}
		Arrays.parallelSort(ancestors);
		int[] closestPortals = new int[n];
		for (int i=0; i<n; i++) {
			closestPortals[i] = ancestors[i]+ ancestors[i]%12;
			System.out.println(closestPortals[i]);
		}
	}
	

	
}
