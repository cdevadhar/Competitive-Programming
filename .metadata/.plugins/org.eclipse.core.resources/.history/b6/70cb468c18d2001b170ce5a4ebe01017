import java.awt.GridLayout;
import javax.swing.*;
public class Gui {
	private JFrame frame;
	public Gui() {
		GridLayout test = new GridLayout(5, 5, 1, 1);
		frame = new JFrame();
		frame.setSize(400,500);
		frame.setLayout(test); 
		frame.setVisible(true);
		
		
	}

	public static void main(String[] args) {
		new Gui();

	}

}
